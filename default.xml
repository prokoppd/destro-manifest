<?xml version="1.0" encoding="UTF-8"?>
<manifest>
  <!-- Default revision for all repos -->
  <default revision="main" remote="origin" sync-j="4" />

  <!-- Define remote repository URL -->
  <remote name="origin" fetch="https://github.com/prokoppd/" />
  <remote name="yocto" fetch="git://git.yoctoproject.org/" />
  <!-- Yocto upstream layer -->
  <project name="poky" path="poky" revision="scarthgap" remote="yocto"/>

  <!-- Your custom layers -->
  <project name="meta-destro" path="meta-destro" revision="main"/>
  <project name="meta-raspberrypi" path="meta-raspberrypi" revision="scarthgap" remote="yocto"/>

  <!-- Optional scripts -->
  <!-- <project name="yocto-scripts" path="scripts" revision="main" /> -->
</manifest>

